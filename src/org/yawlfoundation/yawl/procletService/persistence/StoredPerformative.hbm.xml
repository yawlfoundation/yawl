<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.yawlfoundation.yawl.procletService.persistence.StoredPerformative"
           table="pr_StoredPerformative">

        <id name="pkey" column="pkey" type="long">
   		    <generator class="native"/>
    	</id>

        <property name="timestamp" type="timestamp" column="perftimestamp"/>
        <property name="channel"/>
        <property name="sender"/>
        <property name="receiver"/>
        <property name="action"/>
        <property name="content"/>
        <property name="scope"/>
        <property name="direction"/>
        <property name="entityIDs"/>

    </class>
	
</hibernate-mapping>