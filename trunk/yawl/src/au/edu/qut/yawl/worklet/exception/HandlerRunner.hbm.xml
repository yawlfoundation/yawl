<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="au.edu.qut.yawl.worklet.exception.HandlerRunner" table="Worklet_HandlerRunner">

        <id name="_id" type="integer" access="field">
            <generator class="assigned"/>
        </id>

        <property name="_actionIndex" type="integer"/>
        <property name="_actionCount" type="integer"/>
        <property name="_reasonType" type="integer"/>
        <property name="_isItemSuspended" type="boolean"/>
        <property name="_isCaseSuspended" type="boolean"/>
        <property name="_rdrConcStr" type="text"/>
        <property name="_caseID"/>
        <property name="_suspList" type="text"/>
        <property name="_wirStr" type="text"/>
        <property name="_runningCaseId"/>
        <property name="_workletName"/>
        <property name="_searchPairStr" type="text"/>

    </class>
	
</hibernate-mapping>