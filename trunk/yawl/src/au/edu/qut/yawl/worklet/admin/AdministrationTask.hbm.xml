<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="au.edu.qut.yawl.worklet.admin.AdministrationTask"
           table="Worklet_AdminTask">

       <id name="_wsTaskID" column="id" type="string" access="field">
   		    <generator class="assigned"/>
       </id>
		
       <property name="_caseID"/>
       <property name="_itemID"/>
       <property name="_taskType" type="integer"/>
       <property name="_title"/>
       <property name="_scenario" type="text"/>
       <property name="_process" type="text"/>

    </class>
	
</hibernate-mapping>