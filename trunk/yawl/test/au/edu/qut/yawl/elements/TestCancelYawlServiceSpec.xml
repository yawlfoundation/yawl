<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.yawl.fit.qut.edu.au/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="Beta 7.1"
	xsi:schemaLocation="http://www.yawl.fit.qut.edu.au/ YAWL_SchemaBeta7.1.xsd">
	<specification uri="TestCancelYawlServiceSpec.xml">
		<metaData/>
		<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<decomposition id="OverseeMusic" isRootNet="true" xsi:type="NetFactsType">
			<name>Oversee Music</name>
			<documentation/>
			<processControlElements>
				<inputCondition id="input">
					<flowsInto>
						<nextElementRef id="decideName"/>
					</flowsInto>
				</inputCondition>
				<task id="decideName" naughty="something is wrong!">
					<flowsInto>
						<nextElementRef id="finis"/>
					</flowsInto>
					<join code="and"/>
					<split code="and"/>
					<decomposesTo id="GetPriceViaWS"/>
				</task>
				<outputCondition id="finis">
					<name>Finished case</name>
				</outputCondition>
			</processControlElements>
		</decomposition>
		<decomposition id="GetPriceViaWS" xsi:type="WebServiceGatewayFactsType">
			<yawlService id="http://localhost:8080/yawlWSInvoker/" />
		</decomposition>
	</specification>
</specificationSet>