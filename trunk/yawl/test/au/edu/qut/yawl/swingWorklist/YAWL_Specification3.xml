<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.yawl.fit.qut.edu.au/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.yawl.fit.qut.edu.au/ YAWL_Schema.xsd">
  <specification uri="YAWL_Specification3.xml">
    <metaData />
    <rootNet id="top">
      <processControlElements>
        <inputCondition id="i-top">
          <name>i</name>
          <flowsInto>
            <nextElementRef id="a-top" />
          </flowsInto>
        </inputCondition>
        <task id="a-top">
          <flowsInto>
            <nextElementRef id="c1-top" />
            <predicate>random2()</predicate>
          </flowsInto>
          <flowsInto>
            <nextElementRef id="b-top" />
            <predicate>random1()</predicate>
            <isDefaultFlow />
          </flowsInto>
          <join code="xor" />
          <split code="or" />
          <decomposesTo id="A" />
        </task>
        <condition id="c1-top">
          <name>c1</name>
          <flowsInto>
            <nextElementRef id="c-top" />
          </flowsInto>
        </condition>
        <task id="b-top">
          <flowsInto>
            <nextElementRef id="d-top" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="B" />
        </task>
        <task id="c-top" xsi:type="MultipleInstanceExternalTaskFactsType">
          <flowsInto>
            <nextElementRef id="c2-top" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <removesTokens id="b-top" />
          <removesTokens id="a-top" />
          <decomposesTo id="leaf-c" />
          <minimum>2</minimum>
          <maximum>7</maximum>
          <threshold>5</threshold>
          <creationMode code="dynamic" />
          <miDataInput>
            <expression query="null" />
            <splittingExpression query="null" />
            <formalInputParam>null</formalInputParam>
          </miDataInput>
        </task>
        <condition id="c2-top">
          <name>c2</name>
          <flowsInto>
            <nextElementRef id="d-top" />
          </flowsInto>
        </condition>
        <task id="d-top" xsi:type="MultipleInstanceExternalTaskFactsType">
          <flowsInto>
            <nextElementRef id="o-top" />
          </flowsInto>
          <join code="or" />
          <split code="and" />
          <decomposesTo id="leaf-d" />
          <minimum>1</minimum>
          <maximum>10</maximum>
          <threshold>3</threshold>
          <creationMode code="static" />
          <miDataInput>
            <expression query="null" />
            <splittingExpression query="null" />
            <formalInputParam>null</formalInputParam>
          </miDataInput>
        </task>
        <outputCondition id="o-top">
          <name>o</name>
        </outputCondition>
      </processControlElements>
    </rootNet>
    <decomposition id="I" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="leaf-c" xsi:type="NetFactsType">
      <processControlElements>
        <inputCondition id="i-leaf-c">
          <name>i</name>
          <flowsInto>
            <nextElementRef id="e-leaf-c" />
          </flowsInto>
        </inputCondition>
        <task id="g-leaf-c">
          <flowsInto>
            <nextElementRef id="h-leaf-c" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="G" />
        </task>
        <task id="h-leaf-c">
          <flowsInto>
            <nextElementRef id="o-leaf-c" />
          </flowsInto>
          <join code="or" />
          <split code="and" />
          <decomposesTo id="H" />
        </task>
        <task id="f-leaf-c">
          <flowsInto>
            <nextElementRef id="h-leaf-c" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="F" />
        </task>
        <task id="e-leaf-c">
          <flowsInto>
            <nextElementRef id="f-leaf-c" />
            <predicate>true()</predicate>
            <isDefaultFlow />
          </flowsInto>
          <flowsInto>
            <nextElementRef id="g-leaf-c" />
            <predicate>true()</predicate>
          </flowsInto>
          <join code="xor" />
          <split code="or" />
          <decomposesTo id="E" />
        </task>
        <outputCondition id="o-leaf-c">
          <name>o</name>
        </outputCondition>
      </processControlElements>
    </decomposition>
    <decomposition id="A" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="F" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="H" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="J" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="leaf-d" xsi:type="NetFactsType">
      <processControlElements>
        <inputCondition id="i-leaf-d">
          <name>i</name>
          <flowsInto>
            <nextElementRef id="I-leaf-d" />
          </flowsInto>
        </inputCondition>
        <task id="I-leaf-d">
          <flowsInto>
            <nextElementRef id="o-leaf-d" />
            <predicate>true()</predicate>
          </flowsInto>
          <flowsInto>
            <nextElementRef id="j-leaf-d" />
            <predicate>true()</predicate>
            <isDefaultFlow />
          </flowsInto>
          <join code="xor" />
          <split code="or" />
          <decomposesTo id="I" />
        </task>
        <task id="j-leaf-d">
          <flowsInto>
            <nextElementRef id="o-leaf-d" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="J" />
        </task>
        <outputCondition id="o-leaf-d">
          <name>o</name>
        </outputCondition>
      </processControlElements>
    </decomposition>
    <decomposition id="B" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="G" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="E" xsi:type="WebServiceGatewayFactsType" />
  </specification>
</specificationSet>