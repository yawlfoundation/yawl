<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.yawl.fit.qut.edu.au/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.yawl.fit.qut.edu.au/ d:/yawl/schema/YAWL_Schema.xsd">
	<specification uri="YAWL_Specification4.xml">
		<metaData/>
		<rootNet id="Decompose">
			<processControlElements>
				<inputCondition id="i">
					<name>i</name>
					<flowsInto>
						<nextElementRef id="q"/>
					</flowsInto>
				</inputCondition>
				<task id="q">
					<flowsInto>
						<nextElementRef id="o"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
					<decomposesTo id="Do_Nothing"/>
				</task>
				<outputCondition id="o">
					<name>o</name>
				</outputCondition>
			</processControlElements>
		</rootNet>
		<decomposition id="Sing_a_Song" xsi:type="WebServiceGatewayFactsType"/>
		<decomposition id="Rub_Your_Tummy" xsi:type="WebServiceGatewayFactsType"/>
		<decomposition id="Hop_on_one_Foot" xsi:type="WebServiceGatewayFactsType"/>
		<decomposition id="Pat_Your_Head" xsi:type="WebServiceGatewayFactsType"/>
		<decomposition id="Do_Nothing" xsi:type="NetFactsType">
			<processControlElements>
				<inputCondition id="i-top">
					<name>i</name>
					<flowsInto>
						<nextElementRef id="b-top"/>
					</flowsInto>
				</inputCondition>
				<task id="b-top">
					<flowsInto>
						<nextElementRef id="c-top"/>
					</flowsInto>
					<flowsInto>
						<nextElementRef id="d-top"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
					<decomposesTo id="Pat_Your_Head"/>
				</task>
				<task id="c-top">
					<flowsInto>
						<nextElementRef id="e-top"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
					<removesTokens id="b-top"/>
					<decomposesTo id="Rub_Your_Tummy"/>
				</task>
				<task id="d-top">
					<flowsInto>
						<nextElementRef id="e-top"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
					<decomposesTo id="Hop_on_one_Foot"/>
				</task>
                <task id="e-top">
					<flowsInto>
						<nextElementRef id="o-top"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
					<decomposesTo id="Sing_a_Song"/>
				</task>
				<outputCondition id="o-top">
					<name>o</name>
				</outputCondition>
			</processControlElements>
		</decomposition>
	</specification>
</specificationSet>
