<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.yawl.fit.qut.edu.au/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="Beta 7.1" xsi:schemaLocation="http://www.yawl.fit.qut.edu.au/ d:/yawl/schema/YAWL_SchemaBeta7.1.xsd">
  <specification uri="JythonSpecification.xml">
    <name>Jython Specification</name>
    <documentation />
    <metaData />
    <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" />
    <decomposition id="Net" xsi:type="NetFactsType" isRootNet="true">
      <name>New Net</name>
      <documentation />
      <localVariable>
        <name>Jython__ServiceName</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>Jython</initialValue>
      </localVariable>
      <localVariable>
        <name>Jython__Status</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </localVariable>
      <localVariable>
        <name>Jython__code</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>text::print 'foo'</initialValue>
      </localVariable>
      <localVariable>
        <name>Jython__error</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>text::</initialValue>
      </localVariable>
      <localVariable>
        <name>Jython__output</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>text::</initialValue>
      </localVariable>
      <processControlElements>
        <inputCondition id="start">
          <extension name="YAWL Editor">
            <editinfo xmlns="http://www.nexusbpm.com">
              <rectangle x1="5.0" y1="100.0" x2="50.0" y2="50.0" />
            </editinfo>
          </extension>
          <name>start</name>
          <flowsInto>
            <nextElementRef id="Jython" />
          </flowsInto>
        </inputCondition>
        <task id="Jython">
          <extension name="YAWL Editor">
            <editinfo xmlns="http://www.nexusbpm.com">
              <rectangle x1="105.0" y1="31.0" x2="80.0" y2="70.0" />
            </editinfo>
          </extension>
          <name>Jython</name>
          <flowsInto>
            <nextElementRef id="end" />
          </flowsInto>
          <join code="and" />
          <split code="and" />
          <startingMappings>
            <mapping>
              <expression query="&lt;code&gt;{/Net/Jython__code/text()}&lt;/code&gt;" />
              <mapsTo>code</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;output&gt;{/Net/Jython__output/text()}&lt;/output&gt;" />
              <mapsTo>output</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;ServiceName&gt;{/Net/Jython__ServiceName/text()}&lt;/ServiceName&gt;" />
              <mapsTo>ServiceName</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;error&gt;{/Net/Jython__error/text()}&lt;/error&gt;" />
              <mapsTo>error</mapsTo>
            </mapping>
          </startingMappings>
          <completedMappings>
            <mapping>
              <expression query="&lt;Jython__error&gt;{/Net__Jython/error/text()}&lt;/Jython__error&gt;" />
              <mapsTo>Jython__error</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;Jython__code&gt;{/Net__Jython/code/text()}&lt;/Jython__code&gt;" />
              <mapsTo>Jython__code</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;Jython__output&gt;{/Net__Jython/output/text()}&lt;/Jython__output&gt;" />
              <mapsTo>Jython__output</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;Jython__Status&gt;{/Net__Jython/Status/text()}&lt;/Jython__Status&gt;" />
              <mapsTo>Jython__Status</mapsTo>
            </mapping>
          </completedMappings>
          <decomposesTo id="Net__Jython" />
        </task>
        <outputCondition id="end">
          <extension name="YAWL Editor">
            <editinfo xmlns="http://www.nexusbpm.com">
              <rectangle x1="300.0" y1="100.0" x2="50.0" y2="50.0" />
            </editinfo>
          </extension>
          <name>end</name>
        </outputCondition>
      </processControlElements>
    </decomposition>
    <decomposition id="Net__Jython" xsi:type="WebServiceGatewayFactsType">
      <inputParam>
        <name>code</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>output</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>ServiceName</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>error</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <outputParam>
        <name>Status</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <outputParam>
        <name>code</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <outputParam>
        <name>error</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <outputParam>
        <name>output</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <yawlService id="http://localhost:8080/NexusServiceInvoker/" />
    </decomposition>
  </specification>
</specificationSet>

