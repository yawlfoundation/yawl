<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.yawl.fit.qut.edu.au/ d:/yawl/schema/YAWL_Schema.xsd" xmlns="http://www.yawl.fit.qut.edu.au/" xmlns:mm="www.citi.qut.edu.au/yawl/exampleSchemas/EagerSemantics">
	<specification uri="http://yawlwiki.citi.qut.edu.au/engine/EagerSemantics">
		<metaData/>
		<rootNet id="EagerSemantics">
			<processControlElements>
				<inputCondition id="start">
					<name>Start</name>
					<flowsInto>
						<nextElementRef id="first"/>
					</flowsInto>
				</inputCondition>
				<task id="first">
					<name>FirstTask</name>
					<flowsInto>
						<nextElementRef id="aCondition"/>
                                                <isDefaultFlow/>
					</flowsInto>
					<join code="xor"/>
					<split code="xor"/>
                                </task>
				<condition id="aCondition">
					<flowsInto>
						<nextElementRef id="a"/>
					</flowsInto>
					<flowsInto>
						<nextElementRef id="tau"/>
					</flowsInto>
				</condition>
				<task id="tau">
					<name>a</name>
					<flowsInto>
						<nextElementRef id="finis"/>
                                                <isDefaultFlow/>
					</flowsInto>
					<join code="xor"/>
					<split code="xor"/>
                                        <decomposesTo id="relaxation"/>
				</task>
				<task id="a">
					<flowsInto>
						<nextElementRef id="aCondition"/>
						<isDefaultFlow/>
					</flowsInto>
					<join code="xor"/>
					<split code="xor"/>
				</task>
				<outputCondition id="finis">
					<name>complete</name>
				</outputCondition>
			</processControlElements>
		</rootNet>
		<decomposition id="relaxation" xsi:type="WebServiceGatewayFactsType"/>
	</specification>
</specificationSet>