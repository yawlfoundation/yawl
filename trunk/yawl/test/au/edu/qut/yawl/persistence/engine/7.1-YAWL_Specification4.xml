<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.yawl.fit.qut.edu.au/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="Beta 7.1" xsi:schemaLocation="http://www.yawl.fit.qut.edu.au/ d:/yawl/schema/YAWL_SchemaBeta7.1.xsd">
  <specification uri="YAWL_Specification4.xml">
    <metaData />
    <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" />
    <decomposition id="Decompose" isRootNet="true" xsi:type="NetFactsType">
      <processControlElements>
        <inputCondition id="i">
          <name>i</name>
          <flowsInto>
            <nextElementRef id="q" />
          </flowsInto>
        </inputCondition>
        <task id="q">
          <flowsInto>
            <nextElementRef id="o" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="Do Nothing" />
        </task>
        <outputCondition id="o">
          <name>o</name>
        </outputCondition>
      </processControlElements>
    </decomposition>
    <decomposition id="Sing a Song" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="Rub Your Tummy" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="Hop on one Foot" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="Pat Your Head" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="Do Nothing" xsi:type="NetFactsType">
      <processControlElements>
        <inputCondition id="i-top">
          <name>i</name>
          <flowsInto>
            <nextElementRef id="b-top" />
          </flowsInto>
        </inputCondition>
        <task id="b-top">
          <flowsInto>
            <nextElementRef id="c-top" />
          </flowsInto>
          <flowsInto>
            <nextElementRef id="d-top" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="Pat Your Head" />
        </task>
        <task id="c-top">
          <flowsInto>
            <nextElementRef id="e-top" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <removesTokens id="b-top" />
          <decomposesTo id="Rub Your Tummy" />
        </task>
        <task id="d-top">
          <flowsInto>
            <nextElementRef id="e-top" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="Hop on one Foot" />
        </task>
        <task id="e-top">
          <flowsInto>
            <nextElementRef id="o-top" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="Sing a Song" />
        </task>
        <task id="e-top">
          <flowsInto>
            <nextElementRef id="o-top" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="Sing a Song" />
        </task>
        <outputCondition id="o-top">
          <name>o</name>
        </outputCondition>
      </processControlElements>
    </decomposition>
  </specification>
</specificationSet>

