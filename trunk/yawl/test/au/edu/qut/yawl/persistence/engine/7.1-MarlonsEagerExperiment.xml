<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.citi.qut.edu.au/yawl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="Beta 7.1" xsi:schemaLocation="http://www.citi.qut.edu.au/yawl d:/yawl/schema/YAWL_SchemaBeta7.1.xsd">
  <specification uri="http://yawlwiki.citi.qut.edu.au/engine/EagerSemantics">
    <metaData />
    <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" />
    <decomposition id="EagerSemantics" isRootNet="true" xsi:type="NetFactsType">
      <processControlElements>
        <inputCondition id="start">
          <name>Start</name>
          <flowsInto>
            <nextElementRef id="first" />
          </flowsInto>
        </inputCondition>
        <task id="first">
          <name>FirstTask</name>
          <flowsInto>
            <nextElementRef id="aCondition" />
            <isDefaultFlow />
          </flowsInto>
          <join code="xor" />
          <split code="xor" />
        </task>
        <condition id="aCondition">
          <flowsInto>
            <nextElementRef id="a" />
          </flowsInto>
          <flowsInto>
            <nextElementRef id="tau" />
          </flowsInto>
        </condition>
        <task id="a">
          <flowsInto>
            <nextElementRef id="aCondition" />
            <isDefaultFlow />
          </flowsInto>
          <join code="xor" />
          <split code="xor" />
        </task>
        <task id="tau">
          <name>a</name>
          <flowsInto>
            <nextElementRef id="finis" />
            <isDefaultFlow />
          </flowsInto>
          <join code="xor" />
          <split code="xor" />
          <decomposesTo id="relaxation" />
        </task>
        <outputCondition id="finis">
          <name>complete</name>
        </outputCondition>
      </processControlElements>
    </decomposition>
    <decomposition id="relaxation" xsi:type="WebServiceGatewayFactsType" />
  </specification>
</specificationSet>

