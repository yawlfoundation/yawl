<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.yawl.fit.qut.edu.au/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="Beta 7.1" xsi:schemaLocation="http://www.yawl.fit.qut.edu.au/ d:/yawl/schema/YAWL_SchemaBeta7.1.xsd">
  <specification uri="InternalTest2.xml">
    <metaData>
      <creator>fjellhei</creator>
      <description>No description has been given.</description>
      <version>0.1</version>
    </metaData>
    <schema xmlns="http://www.w3.org/2001/XMLSchema" />
    <decomposition id="New_Net_1" isRootNet="true" xsi:type="NetFactsType">
      <localVariable>
        <name>test1</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>123456</initialValue>
      </localVariable>
      <localVariable>
        <name>test2</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <processControlElements>
        <inputCondition id="InputCondition_1">
          <flowsInto>
            <nextElementRef id="a_3" />
          </flowsInto>
        </inputCondition>
        <task id="a_3">
          <flowsInto>
            <nextElementRef id="b_4" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <startingMappings>
            <mapping>
              <expression query="&lt;a1&gt;{/New_Net_1/test1/text()}&lt;/a1&gt;" />
              <mapsTo>a1</mapsTo>
            </mapping>
          </startingMappings>
          <completedMappings>
            <mapping>
              <expression query="&lt;test2&gt;{/a/a1/text()}&lt;/test2&gt;" />
              <mapsTo>test2</mapsTo>
            </mapping>
          </completedMappings>
          <decomposesTo id="a" />
        </task>
        <task id="b_4">
          <flowsInto>
            <nextElementRef id="OutputCondition_2" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <startingMappings>
            <mapping>
              <expression query="&lt;b1&gt;{/New_Net_1/test2/text()}&lt;/b1&gt;" />
              <mapsTo>b1</mapsTo>
            </mapping>
          </startingMappings>
          <decomposesTo id="b" />
        </task>
        <outputCondition id="OutputCondition_2" />
      </processControlElements>
    </decomposition>
    <decomposition id="a" xsi:type="WebServiceGatewayFactsType">
      <inputParam>
        <name>a1</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <outputParam>
        <name>a1</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
          			<yawlService id="internal://au.edu.qut.yawl.deployment.InternalTestService">
			</yawlService>
    </decomposition>
    <decomposition id="b" xsi:type="WebServiceGatewayFactsType">
      <inputParam>
        <name>b1</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <outputParam>
        <name>b1</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
          			<yawlService id="internal://au.edu.qut.yawl.deployment.InternalTestService">
			</yawlService>
    </decomposition>
  </specification>
</specificationSet>


