<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.citi.qut.edu.au/yawl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="Beta 6" xsi:schemaLocation="http://www.citi.qut.edu.au/yawl d:/yawl/schema/YAWL_SchemaBeta6.xsd">
  <specification uri="creditRating_1.3.ywl">
    <metaData>
      <title>credit rating process</title>
      <creator>C Ouyang</creator>
      <description>A simple process that provides credit rating service.</description>
      <version>0.1</version>
    </metaData>
    <schema xmlns="http://www.w3.org/2001/XMLSchema" />
    <decomposition id="CreditRatingProcess" isRootNet="true" xsi:type="NetFactsType">
      <localVariable>
        <name>fault</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>Bankruptcy</initialValue>
      </localVariable>
      <localVariable>
        <name>rating</name>
        <type>double</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>560</initialValue>
      </localVariable>
      <localVariable>
        <name>ssn</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <processControlElements>
        <inputCondition id="InputCondition_1">
          <flowsInto>
            <nextElementRef id="ReceiveSSN_3" />
          </flowsInto>
        </inputCondition>
        <task id="ReceiveSSN_3">
          <flowsInto>
            <nextElementRef id="DecideRating_4" />
            <isDefaultFlow />
          </flowsInto>
          <flowsInto>
            <nextElementRef id="ReportFault_6" />
            <predicate ordering="0">starts-with(CreditRatingProcess/ssn/text(),'0')</predicate>
          </flowsInto>
          <join code="xor" />
          <split code="xor" />
          <completedMappings>
            <mapping>
              <expression query="&lt;ssn&gt;{/ReceiveSSN/ssn/text()}&lt;/ssn&gt;" />
              <mapsTo>ssn</mapsTo>
            </mapping>
          </completedMappings>
          <decomposesTo id="ReceiveSSN" />
        </task>
        <task id="ReportFault_6">
          <flowsInto>
            <nextElementRef id="_5" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <startingMappings>
            <mapping>
              <expression query="&lt;fault&gt;{/CreditRatingProcess/fault/text()}&lt;/fault&gt;" />
              <mapsTo>fault</mapsTo>
            </mapping>
          </startingMappings>
          <decomposesTo id="ReportFault" />
        </task>
        <task id="DecideRating_4">
          <flowsInto>
            <nextElementRef id="_5" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <startingMappings>
            <mapping>
              <expression query="&lt;rating&gt;number{/CreditRatingProcess/rating/text()}&lt;/rating&gt;" />
              <mapsTo>rating</mapsTo>
            </mapping>
          </startingMappings>
          <decomposesTo id="DecideRating" />
        </task>
        <task id="_5">
          <flowsInto>
            <nextElementRef id="OutputCondition_2" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
        </task>
        <outputCondition id="OutputCondition_2" />
      </processControlElements>
    </decomposition>
    <decomposition id="ReportFault" xsi:type="WebServiceGatewayFactsType">
      <inputParam>
        <name>fault</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
    </decomposition>
    <decomposition id="DecideRating" xsi:type="WebServiceGatewayFactsType">
      <inputParam>
        <name>rating</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
    </decomposition>
    <decomposition id="ReceiveSSN" xsi:type="WebServiceGatewayFactsType">
      <outputParam>
        <name>ssn</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
    </decomposition>
  </specification>
</specificationSet>


