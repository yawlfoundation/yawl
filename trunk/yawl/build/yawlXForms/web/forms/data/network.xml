<network>

<!-- ======================================================================
     CONSTANTS
     ====================================================================== -->
        <constants>
          <ifmodes>
           <ifmode val="ipv4"      label="IPv4 Address"/>
           <ifmode val="ipv6"      label="IPv6 Address"/>
           <ifmode val="dhcp"      label="Dynamic Address from DHCP"/>
           <ifmode val="pppoe"     label="Dynamic Address from PPPoE"/>
           <ifmode val="vlantrunk" label="VLAN Trunk Interface"/>
           <ifmode val="bridge"    label="Bridge Interface"/>
           <ifmode val="carp"      label="CARP Interface"/>
           <ifmode val="chdlc"     label="G.703 HDLC Interface"/>
           <ifmode val="disable"   label="Interface Disabled"/>
          </ifmodes>
          <mediatypes>
           <media  val="auto"           label="Autosense"/>
           <media  val="100baseTX_full" label="100 Mb/s full-duplex"/>
           <media  val="100baseTX_half" label="100 Mb/s half-duplex"/>
           <media  val="10baseT_half"   label="10 Mb/s half-duplex"/>
           <!-- media  val="10baseT_full"   label="10 Mb/s full-duplex"/ -->
          </mediatypes>

        </constants>


</network>


