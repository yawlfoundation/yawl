<?xml version="1.0" encoding="UTF-8"?>
<specificationSet 
xmlns="http://www.citi.qut.edu.au/yawl" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns:editor="http://www.baloney.org"
version="Beta 7.1" 
xsi:schemaLocation="http://www.citi.qut.edu.au/yawl d:/yawl/schema/YAWL_SchemaBeta7.1.xsd http://www.baloney.org YAWL_Editor.xsd"
>
  <specification uri="file:/D:/workspace/yawl_7.1/exampleSpecs/Editor%20Extensions.xml">
    <name>My test specification</name>
    <metaData />
    <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="" />
    <decomposition id="My_test_net" xsi:type="NetFactsType">
      <name>My test net</name>
      <localVariable>
        <name>email_dean.body</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>This is the message body we expect to deliver.</initialValue>
      </localVariable>
      <localVariable>
        <name>email_dean.fromAddress</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>matthew.sandoz@ichg.com</initialValue>
      </localVariable>
      <localVariable>
        <name>email_dean.markupAttachment</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>Not sure what this attachment is for</initialValue>
      </localVariable>
      <localVariable>
        <name>email_dean.subject</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>Did you get that thing I sent you?</initialValue>
      </localVariable>
      <localVariable>
        <name>email_dean.toAddresses</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>dean.mao@ichg.com</initialValue>
      </localVariable>
      <localVariable>
        <name>quote_of_the_day.code</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>print 3.141592653589793</initialValue>
      </localVariable>
      <localVariable>
        <name>quote_of_the_day.error</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>errorout</initialValue>
      </localVariable>
      <localVariable>
        <name>quote_of_the_day.output</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>systemout</initialValue>
      </localVariable>
      <processControlElements>
        <inputCondition id="start">
		<extension name="YAWL Editor">
			<editor:editinfo>
				<editor:point x="50" y="50"/>
			</editor:editinfo>
		</extension>
          <flowsInto>
            <nextElementRef id="quote_of_the_day" />
          </flowsInto>
        </inputCondition>
        <task id="quote_of_the_day">
		<extension name="YAWL Editor">
			<editor:editinfo>
				<editor:point x="300" y="50"/>
			</editor:editinfo>
		</extension>
          <name>quote of the day</name>
          <flowsInto>
            <nextElementRef id="email_dean" />
          </flowsInto>
          <join code="and" />
          <split code="and" />
          <startingMappings>
            <mapping>
              <expression query="/My_test_net/quote_of_the_day.code" />
              <mapsTo>code</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;YawlWSInvokerOperationName&gt;execute&lt;/YawlWSInvokerOperationName&gt;" />
              <mapsTo>YawlWSInvokerOperationName</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;YawlWSInvokerWSDLLocation&gt;http://localhost:8080/JythonService/services/JythonService?wsdl&lt;/YawlWSInvokerWSDLLocation&gt;" />
              <mapsTo>YawlWSInvokerWSDLLocation</mapsTo>
            </mapping>
            <mapping>
              <expression query="/My_test_net/quote_of_the_day.output" />
              <mapsTo>output</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;YawlWSInvokerPortName&gt;&lt;/YawlWSInvokerPortName&gt;" />
              <mapsTo>YawlWSInvokerPortName</mapsTo>
            </mapping>
            <mapping>
              <expression query="/My_test_net/quote_of_the_day.error" />
              <mapsTo>error</mapsTo>
            </mapping>
          </startingMappings>
          <completedMappings>
            <mapping>
              <expression query="/com.nexusbpm.editor.component.JythonComponent/code" />
              <mapsTo>quote_of_the_day.code</mapsTo>
            </mapping>
            <mapping>
              <expression query="/com.nexusbpm.editor.component.JythonComponent/output" />
              <mapsTo>quote_of_the_day.output</mapsTo>
            </mapping>
            <mapping>
              <expression query="/com.nexusbpm.editor.component.JythonComponent/error" />
              <mapsTo>quote_of_the_day.error</mapsTo>
            </mapping>
          </completedMappings>
          <decomposesTo id="com.nexusbpm.editor.component.JythonComponent" />
        </task>
        <task id="email_dean">
		<extension name="YAWL Editor">
			<editor:editinfo>
				<editor:point x="50" y="300"/>
			</editor:editinfo>
		</extension>
          <name>email dean</name>
          <flowsInto>
            <nextElementRef id="end" />
          </flowsInto>
          <join code="and" />
          <split code="and" />
          <startingMappings>
            <mapping>
              <expression query="/My_test_net/email_dean.markupAttachment" />
              <mapsTo>markupAttachment</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;YawlWSInvokerOperationName&gt;execute&lt;/YawlWSInvokerOperationName&gt;" />
              <mapsTo>YawlWSInvokerOperationName</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;YawlWSInvokerWSDLLocation&gt;http://localhost:8080/JythonService/services/JythonService?wsdl&lt;/YawlWSInvokerWSDLLocation&gt;" />
              <mapsTo>YawlWSInvokerWSDLLocation</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;YawlWSInvokerPortName&gt;&lt;/YawlWSInvokerPortName&gt;" />
              <mapsTo>YawlWSInvokerPortName</mapsTo>
            </mapping>
            <mapping>
              <expression query="/My_test_net/email_dean.toAddresses" />
              <mapsTo>toAddresses</mapsTo>
            </mapping>
            <mapping>
              <expression query="/My_test_net/email_dean.fromAddress" />
              <mapsTo>fromAddress</mapsTo>
            </mapping>
            <mapping>
              <expression query="/My_test_net/email_dean.subject" />
              <mapsTo>subject</mapsTo>
            </mapping>
            <mapping>
              <expression query="/My_test_net/email_dean.body" />
              <mapsTo>body</mapsTo>
            </mapping>
          </startingMappings>
          <completedMappings>
            <mapping>
              <expression query="/com.nexusbpm.editor.component.EmailSenderComponent/body" />
              <mapsTo>email_dean.body</mapsTo>
            </mapping>
            <mapping>
              <expression query="/com.nexusbpm.editor.component.EmailSenderComponent/subject" />
              <mapsTo>email_dean.subject</mapsTo>
            </mapping>
            <mapping>
              <expression query="/com.nexusbpm.editor.component.EmailSenderComponent/toAddresses" />
              <mapsTo>email_dean.toAddresses</mapsTo>
            </mapping>
            <mapping>
              <expression query="/com.nexusbpm.editor.component.EmailSenderComponent/markupAttachment" />
              <mapsTo>email_dean.markupAttachment</mapsTo>
            </mapping>
            <mapping>
              <expression query="/com.nexusbpm.editor.component.EmailSenderComponent/fromAddress" />
              <mapsTo>email_dean.fromAddress</mapsTo>
            </mapping>
          </completedMappings>
          <decomposesTo id="com.nexusbpm.editor.component.EmailSenderComponent" />
        </task>
        <outputCondition id="end">
		<extension name="YAWL Editor">
			<editor:editinfo>
				<editor:point x="300" y="300"/>
			</editor:editinfo>
		</extension>
        </outputCondition>
      </processControlElements>
    </decomposition>
    <decomposition id="com.nexusbpm.editor.component.EmailSenderComponent" xsi:type="WebServiceGatewayFactsType">
      <name>[send mail support]</name>
      <inputParam>
        <name>fromAddress</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>toAddresses</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>subject</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>body</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>markupAttachment</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <outputParam>
        <name>fromAddress</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <outputParam>
        <name>toAddresses</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <outputParam>
        <name>subject</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <outputParam>
        <name>body</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <outputParam>
        <name>markupAttachment</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
    </decomposition>
    <decomposition id="com.nexusbpm.editor.component.JythonComponent" xsi:type="WebServiceGatewayFactsType">
      <name>[jython scripting support]</name>
      <inputParam>
        <name>YawlWSInvokerWSDLLocation</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>YawlWSInvokerOperationName</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>YawlWSInvokerPortName</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>code</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>output</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <name>error</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <outputParam>
        <name>code</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <outputParam>
        <name>output</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <outputParam>
        <name>error</name>
        <type>string</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <yawlService id="http://localhost:8080/yawlWSInvoker/" />
    </decomposition>
  </specification>
</specificationSet>

