<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="org.yawlfoundation.yawl.engine.YSpecFile" table="specs">

    <composite-id name="specid" class="org.yawlfoundation.yawl.engine.P_YSpecFileID">
      <key-property name="version"/>
      <key-property name="id"/>
    </composite-id>
	<property name="XML" column="specs" type="text"/>

	</class>

</hibernate-mapping>
