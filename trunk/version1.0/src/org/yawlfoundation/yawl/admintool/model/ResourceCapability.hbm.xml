<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="org.yawlfoundation.yawl.admintool.model.ResourceCapability" table="ResourceCapability">

        <composite-id>
            <key-many-to-one name="ResourceID" class="org.yawlfoundation.yawl.admintool.model.Resource" column="ResourceID"/>
            <key-many-to-one name="CapabilityDesc" class="org.yawlfoundation.yawl.admintool.model.Capability" column="CapabilityDesc"/>
        </composite-id>

    </class>

</hibernate-mapping>