<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.yawlfoundation.yawl.worklet.support.WorkletEvent"
           table="Worklet_EventLog">

       <id name="_id" type="long" access="field">
   		    <generator class="assigned"/>
       </id>
		
       <property name="_event"/>
       <property name="_caseId"/>
       <property name="_specId"/>
       <property name="_taskId"/>
       <property name="_parentCaseId"/>
       <property name="_stamp"/>
       <property name="_xType" type="int"/>

    </class>
	
</hibernate-mapping>