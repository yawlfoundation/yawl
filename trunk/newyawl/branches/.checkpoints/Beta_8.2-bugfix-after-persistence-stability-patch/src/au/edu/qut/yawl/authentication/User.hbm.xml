<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="au.edu.qut.yawl.authentication.User" table="ResSerPosID">

        <id name="userID" column="ID" type="string" access="field">
            <generator class="assigned"/>
        </id>

        <property name="isAdmin" column="IsAdministrator" access="field"/>

        <property name="password" column="password" access="field"/>

    </class>

</hibernate-mapping>