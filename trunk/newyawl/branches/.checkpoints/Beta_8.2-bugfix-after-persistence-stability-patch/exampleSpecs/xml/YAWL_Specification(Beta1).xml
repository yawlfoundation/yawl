<?xml version="1.0" encoding="UTF-8"?>
<YAWLSpecificationSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="YAWL_Schema.xsd">
	<YAWLSpecification>
		<uri uriName="http://yawlwiki.citi.qut.edu.au/somewherethere"/>
		<!--specID id="something"/-->
		<metaData>
			<creator>
				<name>Lachlan Aldred</name>
				<address>
					<numAndStreet>
						<number>3/60</number>
						<street>Edmund St</street>
					</numAndStreet>
					<postOrZipCode>4551</postOrZipCode>
					<country>Australia</country>
				</address>
			</creator>
		</metaData>
		<net name="top">
			<inputCondition id="i-top">
				<flowsInto>
					<nextElementRef id="a-top"/>
				</flowsInto>
				<label>i</label>
			</inputCondition>
			<atomicTask id="a-top">
				<flowsInto>
					<nextElementRef id="b-top"/>
					<predicate xpath="random1()"/>
				</flowsInto>
				<flowsInto>
					<nextElementRef id="c1-top"/>
					<predicate xpath="random2()"/>
				</flowsInto>
				<join code="xor"/>
				<split code="or"/>
				<activityRef id="A"/>
			</atomicTask>
			<atomicTask id="b-top">
				<flowsInto>
					<nextElementRef id="d-top"/>
				</flowsInto>
				<join code="xor"/>
				<split code="and"/>
				<activityRef id="B"/>
			</atomicTask>
			<condition id="c1-top">
				<flowsInto>
					<nextElementRef id="c-top"/>
				</flowsInto>
				<label>c1</label>
			</condition>
			<compositeTask id="c-top">
				<flowsInto>
					<nextElementRef id="c2-top"/>
				</flowsInto>
				<join code="xor"/>
				<split code="and"/>
				<removesTokens id="b-top"/>
				<removesTokens id="a-top"/>
				<multipleInstances>
					<minimum>2</minimum>
					<maximum>7</maximum>
					<threshold>5</threshold>
					<creationMode code="dynamic"/>
				</multipleInstances>
				<decomposesTo name="leaf-c"/>
			</compositeTask>
			<condition id="c2-top">
				<flowsInto>
					<nextElementRef id="d-top"/>
				</flowsInto>
				<label>c2</label>
			</condition>
			<compositeTask id="d-top">
				<flowsInto>
					<nextElementRef id="o-top"/>
				</flowsInto>
				<join code="or"/>
				<split code="and"/>
				<multipleInstances>
					<minimum>1</minimum>
					<maximum>10</maximum>
					<threshold>3</threshold>
					<creationMode code="static"/>
				</multipleInstances>
				<decomposesTo name="leaf-d"/>
			</compositeTask>
			<outputCondition id="o-top">
				<label>o</label>
			</outputCondition>
		</net>
		<net name="leaf-c">
			<inputCondition id="i-leaf-c">
				<flowsInto>
					<nextElementRef id="e-leaf-c"/>
				</flowsInto>
				<label>i</label>
			</inputCondition>
			<atomicTask id="e-leaf-c">
				<flowsInto>
					<nextElementRef id="f-leaf-c"/>
					<predicate xpath="true()"/>
				</flowsInto>
				<flowsInto>
					<nextElementRef id="g-leaf-c"/>
					<predicate xpath="true()"/>
				</flowsInto>
				<join code="xor"/>
				<split code="or"/>
				<activityRef id="E"/>
			</atomicTask>
			<atomicTask id="f-leaf-c">
				<flowsInto>
					<nextElementRef id="h-leaf-c"/>
				</flowsInto>
				<join code="xor"/>
				<split code="and"/>
				<activityRef id="F"/>
			</atomicTask>
			<atomicTask id="g-leaf-c">
				<flowsInto>
					<nextElementRef id="h-leaf-c"/>
				</flowsInto>
				<join code="xor"/>
				<split code="and"/>
				<activityRef id="G"/>
			</atomicTask>
			<atomicTask id="h-leaf-c">
				<flowsInto>
					<nextElementRef id="o-leaf-c"/>
				</flowsInto>
				<join code="or"/>
				<split code="and"/>
				<activityRef id="H"/>
			</atomicTask>
			<outputCondition id="o-leaf-c">
				<label>o</label>
			</outputCondition>
		</net>
		<net name="leaf-d">
			<inputCondition id="i-leaf-d">
				<flowsInto>
					<nextElementRef id="I-leaf-d"/>
				</flowsInto>
				<label>i</label>
			</inputCondition>
			<atomicTask id="I-leaf-d">
				<flowsInto>
					<nextElementRef id="j-leaf-d"/>
					<predicate xpath="true()"/>
				</flowsInto>
				<flowsInto>
					<nextElementRef id="o-leaf-d"/>
					<predicate xpath="true()"/>
				</flowsInto>
				<join code="xor"/>
				<split code="or"/>
				<activityRef id="I"/>
			</atomicTask>
			<atomicTask id="j-leaf-d">
				<flowsInto>
					<nextElementRef id="o-leaf-d"/>
				</flowsInto>
				<join code="xor"/>
				<split code="and"/>
				<activityRef id="J"/>
			</atomicTask>
			<outputCondition id="o-leaf-d">
				<label>o</label>
			</outputCondition>
		</net>
		<localDataType id="localType_1" xmlns:xs="http://www.w3.org/2001/XMLSchema">
			<xs:complexType name="AussieAddress">
				<xs:sequence>
					<xs:element name="street">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="\d*[/|-]*\d+\s+\D+\s+[S|s|R|r|A|a|C|c|B|b|o](treet|oad|venue|t|d|ve|rescent|lvd|ther)"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="suburb" type="xs:NMTOKENS"/>
					<xs:element name="postcode">
						<xs:simpleType>
							<xs:restriction base="xs:positiveInteger">
								<xs:minInclusive value="1000"/>
								<xs:maxInclusive value="9999"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="state">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="(QLD|VIC|NSW|SA|NT|ACT|TAS|WA)"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="country">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="[a|A]ustralia"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</localDataType>
		<activity id="A">
			<role roleName="Joe"/>
			<role roleName="Fred"/>
			<application code="Application1"/>
		</activity>
		<activity id="B">
			<application code="Application2"/>
		</activity>
		<activity id="E">
			<role roleName="A role1"/>
		</activity>
		<activity id="F">
			<application code="Application3"/>
		</activity>
		<activity id="G">
			<role roleName="A role2"/>
		</activity>
		<activity id="H">
			<application code="Application4"/>
			<application code="Application5"/>
		</activity>
		<activity id="I">
			<role roleName="A role3"/>
		</activity>
		<activity id="J">
			<application code="Application6"/>
		</activity>
	</YAWLSpecification>
</YAWLSpecificationSet>
