            </mapping>
            <mapping>
              <expression query="&lt;Notes&gt;{/Treat_BP/Notes/text()}&lt;/Notes&gt;" />
              <mapsTo>Notes</mapsTo>
            </mapping>
          </completedMappings>
          <decomposesTo id="Treat_BP" />
        </task>
        <outputCondition id="1_OutputCondition" />
      </processControlElements>
