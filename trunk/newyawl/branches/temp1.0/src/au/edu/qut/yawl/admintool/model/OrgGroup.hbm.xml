<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="au.edu.qut.yawl.admintool.model.OrgGroup" table="OrgGroup">

        <id name="OrgGroupName" type="string">
            <generator class="assigned"/>
        </id>

        <property name="OrgGroupType" type="string" />
        <property name="WorkingDayStartsAt" type="integer" />
        <property name="WorkingDayFinishesAt" type="integer" />

    </class>

</hibernate-mapping>