<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

	<class name="au.edu.qut.yawl.engine.P_YIdentifier" table="YIdentifiers">

	    <id name="_idString" column="id" type="string">
    		<generator class="assigned"/>
    	   </id>
   	
          <list name="_children">
    		<key column="parent"/>
    		<index column="ix"/>
	    	<one-to-many class="au.edu.qut.yawl.engine.P_YIdentifier"/>
	    </list>

	    <list name="locationNames" table="LOCATIONS" access="field">
	      <key column="id_key"/>
	      <index column="ix"/>
	      <element column="condition" type="string"/>
	    </list>

	</class>
	
</hibernate-mapping>