<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="au.edu.qut.yawl.admintool.model.Position" table="Position">

        <id name="positionID" column="positionID" type="string">
            <generator class="assigned"/>
        </id>

        <property name="positionName" />

        <many-to-one name="BelongsToOrgGroup" class="au.edu.qut.yawl.admintool.model.OrgGroup" />

    </class>

</hibernate-mapping>