                <element name="camRoll" type="string" />
                <element name="totalCansNumber" type="integer" />
                <element name="magNumber" type="string" />
              </sequence>
            </complexType>
          </element>
          <element maxOccurs="unbounded" name="sceneInfo">
            <complexType>
              <sequence>
                <element name="scene" type="string" />
                <element maxOccurs="unbounded" name="slateInfo">
                  <complexType>
