                    <sequence>
                      <element name="slate" type="integer" />
                      <element maxOccurs="unbounded" name="takeInfo">
                        <complexType>
                          <sequence>
                            <element name="take" type="integer" />
                            <element name="timecode" type="time" />
                            <element name="remarks" type="string" />
                            <element name="camRoll" type="string" />
                          </sequence>
                        </complexType>
                      </element>
                    </sequence>
                  </complexType>
                </element>
              </sequence>
            </complexType>
          </element>
        </sequence>
      </complexType>
      <complexType name="cameraInfoType">
        <sequence>
          <element name="sheetNumber" type="integer" />
          <element name="projectNumber" type="string" />
          <element name="studios_location" type="string" />
