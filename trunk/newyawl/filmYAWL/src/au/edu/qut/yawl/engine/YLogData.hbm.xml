<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>


	<class name="au.edu.qut.yawl.engine.YLogData" table="logdata">

       <id name="rowkey" column="rowkey" type="string" access="field">
   		<generator class="uuid.hex"/>
    	</id>

    	<property name="eventid" column="eventid" type="string" access="field"/>

    	<property name="port" column="port" type="string" access="field"/>

    	<property name="value" column="value" type="text" access="field"/>

    	<property name="io" column="io" type="string" access="field"/>

	</class>
	
</hibernate-mapping>