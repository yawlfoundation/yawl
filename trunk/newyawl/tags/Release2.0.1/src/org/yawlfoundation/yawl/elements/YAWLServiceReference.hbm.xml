<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.yawlfoundation.yawl.elements.YAWLServiceReference" table="services">

       <id name="_yawlServiceID" column="serviceid" type="string" access="field">
   		<generator class="assigned"/>
    	</id>

        <property name="_serviceName" column="serviceName" access="field"/>
        <property name="_documentation" column="documentation" access="field"/>
        <property name="_assignable" column="assignable" type="boolean" access="field"/>

	</class>
	
</hibernate-mapping>