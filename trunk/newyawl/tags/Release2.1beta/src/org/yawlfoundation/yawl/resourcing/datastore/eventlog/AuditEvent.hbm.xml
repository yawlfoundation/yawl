<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.yawlfoundation.yawl.resourcing.datastore.eventlog">

	<class name="AuditEvent" table="rs_AuditLog">

        <id name="_id"
            column="rowid"
            type="long">
            <generator class="native"/>
        </id>

        <property name="_userid" column="userID"/>
        <property name="_event" column="event"/>
        <property name="_timeStamp" column="eventTime" type="long"/>       
     </class>
	
</hibernate-mapping>
