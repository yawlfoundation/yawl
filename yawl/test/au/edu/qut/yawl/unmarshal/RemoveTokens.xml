<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.yawl.fit.qut.edu.au/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.yawl.fit.qut.edu.au/ d:/yawl/schema/YAWL_Schema.xsd">
	<specification uri="RemoveTokens.xml">
		<metaData/>
		<rootNet id="Remove">
			<processControlElements>
				<inputCondition id="i">
					<flowsInto>
						<nextElementRef id="t1"/>
					</flowsInto>
				</inputCondition>
				<task id="t1">
					<flowsInto>
						<nextElementRef id="t2"/>
					</flowsInto>
					<join code="and"/>
					<split code="and"/>
					<decomposesTo id="A nothing task t1"/>
				</task>
				<task id="t2">
					<flowsInto>
						<nextElementRef id="o"/>
					</flowsInto>
					<join code="and"/>
					<split code="and"/>
					<removesTokens id="t1"/>
					<removesTokensFromFlow>
						<flowSource id="t1"/>
						<flowDestination id="t2"/>
					</removesTokensFromFlow>
					<decomposesTo id="A nothing task t2"/>
				</task>
				<outputCondition id="o"/>
			</processControlElements>
		</rootNet>
		<decomposition id="a Nothing decomposition" xsi:type="NetFactsType">
			<processControlElements>
				<inputCondition id="1">
					<flowsInto>
						<nextElementRef id="2"></nextElementRef>
					</flowsInto>
				</inputCondition>
				<task id="2">
					<flowsInto>
						<nextElementRef id="3"></nextElementRef>
					</flowsInto>
					<join code="and"></join>
					<split code="and"></split>
					<removesTokens id="1"/>
					<removesTokensFromFlow>
						<flowSource id="2"/>
						<flowDestination id="2"/>
					</removesTokensFromFlow>
				</task>
				<outputCondition id="3"></outputCondition>
			</processControlElements>
		</decomposition>
		<decomposition id="A nothing task t1" xsi:type="WebServiceGatewayFactsType"/>
		<decomposition id="A nothing task t2" xsi:type="WebServiceGatewayFactsType"/>
	</specification>
</specificationSet>
