<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.yawl.fit.qut.edu.au/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.yawl.fit.qut.edu.au/ d:/yawl/schema/YAWL_Schema.xsd">
	<specification uri="TestOrJoin">
		<metaData/>
		<rootNet id="Main">
			<processControlElements>
				<inputCondition id="input">
					<flowsInto>
						<nextElementRef id="A"/>
					</flowsInto>
				</inputCondition>
				<task id="A">
					<flowsInto>
						<nextElementRef id="A"/>
					</flowsInto>
					<flowsInto>
						<nextElementRef id="C1"/>
					</flowsInto>
					<flowsInto>
						<nextElementRef id="F"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
					<decomposesTo id="A"/>
				</task>
				<condition id="C1">
					<flowsInto>
						<nextElementRef id="C"/>
					</flowsInto>
					<flowsInto>
						<nextElementRef id="B"/>
					</flowsInto>
				</condition>
				<task id="B">
					<flowsInto>
						<nextElementRef id="D"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
					<decomposesTo id="B"/>
				</task>
				<task id="C">
					<flowsInto>
						<nextElementRef id="D"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
					<decomposesTo id="C"/>
				</task>
				<task id="F">
					<flowsInto>
						<nextElementRef id="C2"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
					<decomposesTo id="F"/>
				</task>
				<condition id="C2">
					<flowsInto>
						<nextElementRef id="E"/>
					</flowsInto>
				</condition>
				<task id="E">
					<flowsInto>
						<nextElementRef id="1"/>
					</flowsInto>
					<join code="or"/>
					<split code="and"/>
					<decomposesTo id="E"/>
				</task>
				<task id="D">
					<flowsInto>
						<nextElementRef id="C3"/>
					</flowsInto>
					<join code="and"/>
					<split code="and"/>
					<decomposesTo id="D"/>
				</task>
				<condition id="C3">
					<flowsInto>
						<nextElementRef id="E"/>
					</flowsInto>
				</condition>
				<outputCondition id="1"/>
			</processControlElements>
		</rootNet>
		<decomposition id="D" xsi:type="WebServiceGatewayFactsType"/>
		<decomposition id="A" xsi:type="WebServiceGatewayFactsType"/>
		<decomposition id="F" xsi:type="WebServiceGatewayFactsType"/>
		<decomposition id="C" xsi:type="WebServiceGatewayFactsType"/>
		<decomposition id="B" xsi:type="WebServiceGatewayFactsType"/>
		<decomposition id="E" xsi:type="WebServiceGatewayFactsType"/>
	</specification>
</specificationSet>
