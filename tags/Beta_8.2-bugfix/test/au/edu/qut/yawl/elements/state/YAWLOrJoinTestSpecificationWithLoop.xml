<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.citi.qut.edu.au/yawl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.citi.qut.edu.au/yawl YAWL_Schema.xsd">
  <specification uri="YAWLOrJoinTestSpecificationWithLoop.xml">
    <metaData />
    <rootNet id="top">
      <processControlElements>
        <inputCondition id="i-top">
          <name>i</name>
          <flowsInto>
            <nextElementRef id="q" />
          </flowsInto>
        </inputCondition>
        <task id="d">
          <flowsInto>
            <nextElementRef id="o-top" />
          </flowsInto>
          <join code="or" />
          <split code="and" />
          <decomposesTo id="D" />
        </task>
        <condition id="cC">
          <name>cC</name>
          <flowsInto>
            <nextElementRef id="c" />
          </flowsInto>
        </condition>
        <task id="c">
          <flowsInto>
            <nextElementRef id="q" />
          </flowsInto>
          <flowsInto>
            <nextElementRef id="o-top" />
          </flowsInto>
          <join code="xor" />
          <split code="xor" />
          <decomposesTo id="C" />
        </task>
        <task id="b">
          <flowsInto>
            <nextElementRef id="d" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="B" />
        </task>
        <task id="q">
          <flowsInto>
            <nextElementRef id="cC" />
          </flowsInto>
          <flowsInto>
            <nextElementRef id="cA" />
          </flowsInto>
          <flowsInto>
            <nextElementRef id="cB" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="A" />
        </task>
        <task id="a">
          <flowsInto>
            <nextElementRef id="d" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <decomposesTo id="A" />
        </task>
        <condition id="cA">
          <name>cA</name>
          <flowsInto>
            <nextElementRef id="a" />
          </flowsInto>
        </condition>
        <condition id="cB">
          <name>cB</name>
          <flowsInto>
            <nextElementRef id="b" />
          </flowsInto>
        </condition>
        <outputCondition id="o-top">
          <name>o</name>
        </outputCondition>
      </processControlElements>
    </rootNet>
    <decomposition id="D" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="A" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="C" xsi:type="WebServiceGatewayFactsType" />
    <decomposition id="B" xsi:type="WebServiceGatewayFactsType" />
  </specification>
</specificationSet>