<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

	<class name="au.edu.qut.yawl.engine.YCaseData" table="CaseDataDocument">

       <id name="id" column="net_id" type="string" access="field">
   		<generator class="assigned"/>
    	</id>
		

    	<property name="data" column="casedata" type="text" access="field"/>
        <property name="executionState" column="executionState" type="integer" />

	</class>
	
</hibernate-mapping>